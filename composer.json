{
    "name":              "hostnet/asset-bundle",
    "type":              "symfony-bundle",
    "description":       "Allows the configuration of the hostnet/asset-lib package in your Symfony application.",
    "license":           "MIT",
    "minimum-stability": "dev",
    "prefer-stable":     true,
    "authors": [
        {
            "name":  "Yannick de Lange",
            "email": "ydelange@hostnet.nl"
        }
    ],
    "require": {
        "php":                      ">=7.1",
        "hostnet/asset-lib":        "^0.1.3",
        "symfony/config":           "^3.3||^4.0",
        "symfony/framework-bundle": "^3.3||^4.0",
        "symfony/http-foundation":  "^3.3||^4.0",
        "symfony/http-kernel":      "^3.3||^4.0",
        "symfony/process":          "^3.3||^4.0"
    },
    "require-dev": {
        "hostnet/phpcs-tool": "^6.0.2",
        "phpunit/phpunit":    "^6.3",
        "symfony/yaml":       "^3.3||^4.0",
        "twig/twig":          "^1.38.2||^2.7.2"
    },
    "conflict": {
        "phpdocumentor/type-resolver": "<0.3",
        "twig/twig":                   "<1.26"
    },
    "autoload": {
        "psr-4": {
            "Hostnet\\Bundle\\AssetBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hostnet\\Bundle\\AssetBundle\\": "test/"
        }
    },
    "archive": {
        "exclude": [
            "/test"
        ]
    }
}
